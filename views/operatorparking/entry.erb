<!DOCTYPE html>
<%#include "operatorparking.h" %>
<% tfetch(QVariantMap, operatorParking); %>
<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>New Operator Parking</h1>

<%== formTag(urla("create"), Tf::Post) %>
  <p>
    <label>Operator ID<br /><input name="operatorParking[operatorId]" value="<%= operatorParking["operatorId"] %>" /></label>
  </p>
  <p>
    <label>Parking ID<br /><input name="operatorParking[parkingId]" value="<%= operatorParking["parkingId"] %>" /></label>
  </p>
  <p>
    <label>Status<br /><input name="operatorParking[status]" value="<%= operatorParking["status"] %>" /></label>
  </p>
  <p>
    <input type="submit" value="Create" />
  </p>
</form>

<%== linkTo("Back", urla("index")) %>

</body>
</html>

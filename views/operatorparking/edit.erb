<!DOCTYPE html>
<%#include "operatorparking.h" %>
<% tfetch(QVariantMap, operatorParking); %>
<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>Editing Operator Parking</h1>

<%== formTag(urla("save", operatorParking["id"]), Tf::Post) %>
  <p>
    <label>ID<br /><input type="text" name="operatorParking[id]" value="<%= operatorParking["id"] %>" readonly="readonly" /></label>
  </p>
  <p>
    <label>Operator ID<br /><input type="text" name="operatorParking[operatorId]" value="<%= operatorParking["operatorId"] %>" /></label>
  </p>
  <p>
    <label>Parking ID<br /><input type="text" name="operatorParking[parkingId]" value="<%= operatorParking["parkingId"] %>" /></label>
  </p>
  <p>
    <label>Status<br /><input type="text" name="operatorParking[status]" value="<%= operatorParking["status"] %>" /></label>
  </p>
  <p>
    <input type="submit" value="Update" />
  </p>
</form>

<%== linkTo("Show", urla("show", operatorParking["id"])) %> |
<%== linkTo("Back", urla("index")) %>
</body>
</html>

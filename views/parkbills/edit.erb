<!DOCTYPE html>
<%#include "parkbills.h" %>
<% tfetch(QVariantMap, parkBills); %>
<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>Editing Park Bills</h1>

<%== formTag(urla("save", parkBills["id"]), Tf::Post) %>
  <p>
    <label>ID<br /><input type="text" name="parkBills[id]" value="<%= parkBills["id"] %>" readonly="readonly" /></label>
  </p>
  <p>
    <label>Session ID<br /><input type="text" name="parkBills[sessionId]" value="<%= parkBills["sessionId"] %>" /></label>
  </p>
  <p>
    <label>Source<br /><input type="text" name="parkBills[source]" value="<%= parkBills["source"] %>" /></label>
  </p>
  <p>
    <label>Amount<br /><input type="text" name="parkBills[amount]" value="<%= parkBills["amount"] %>" /></label>
  </p>
  <p>
    <label>Status<br /><input type="text" name="parkBills[status]" value="<%= parkBills["status"] %>" /></label>
  </p>
  <p>
    <input type="submit" value="Update" />
  </p>
</form>

<%== linkTo("Show", urla("show", parkBills["id"])) %> |
<%== linkTo("Back", urla("index")) %>
</body>
</html>

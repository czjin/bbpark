<!DOCTYPE html>
<%#include "parkprepay.h" %>
<% tfetch(QVariantMap, parkPrepay); %>
<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>Editing Park Prepay</h1>

<%== formTag(urla("save", parkPrepay["id"]), Tf::Post) %>
  <p>
    <label>ID<br /><input type="text" name="parkPrepay[id]" value="<%= parkPrepay["id"] %>" readonly="readonly" /></label>
  </p>
  <p>
    <label>User ID<br /><input type="text" name="parkPrepay[userId]" value="<%= parkPrepay["userId"] %>" /></label>
  </p>
  <p>
    <label>Session ID<br /><input type="text" name="parkPrepay[sessionId]" value="<%= parkPrepay["sessionId"] %>" /></label>
  </p>
  <p>
    <label>Prepaid<br /><input type="text" name="parkPrepay[prepaid]" value="<%= parkPrepay["prepaid"] %>" /></label>
  </p>
  <p>
    <input type="submit" value="Update" />
  </p>
</form>

<%== linkTo("Show", urla("show", parkPrepay["id"])) %> |
<%== linkTo("Back", urla("index")) %>
</body>
</html>

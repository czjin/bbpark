<!DOCTYPE html>
<%#include "parks.h" %>
<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>

<h1>Listing Parks</h1>

<%== linkTo("New entry", urla("entry")) %><br />
<br />
<table border="1" cellpadding="5" style="border: 1px #d0d0d0 solid; border-collapse: collapse;">
  <tr>
    <th>ID</th>
    <th>Session ID</th>
    <th>User ID</th>
    <th>Vehicle ID</th>
    <th>Parking ID</th>
    <th>Parking Space No</th>
    <th>Start Time</th>
    <th>End Time</th>
    <th>Park Fee</th>
    <th>Prepaid</th>
    <th>Order Duration</th>
    <th>Status</th>
    <th>Pay Mode</th>
    <th>Pay Source</th>
    <th>Qrcode Path</th>
    <th>Memo</th>
  </tr>
<% tfetch(QList<Parks>, parksList); %>
<% for (const auto &i : parksList) { %>
  <tr>
    <td><%= i.id() %></td>
    <td><%= i.sessionId() %></td>
    <td><%= i.userId() %></td>
    <td><%= i.vehicleId() %></td>
    <td><%= i.parkingId() %></td>
    <td><%= i.parkingSpaceNo() %></td>
    <td><%= i.startTime() %></td>
    <td><%= i.endTime() %></td>
    <td><%= i.parkFee() %></td>
    <td><%= i.prepaid() %></td>
    <td><%= i.orderDuration() %></td>
    <td><%= i.status() %></td>
    <td><%= i.payMode() %></td>
    <td><%= i.paySource() %></td>
    <td><%= i.qrcodePath() %></td>
    <td><%= i.memo() %></td>
    <td>
      <%== linkTo("Show", urla("show", i.id())) %>
      <%== linkTo("Edit", urla("edit", i.id())) %>
      <%== linkTo("Remove", urla("remove", i.id()), Tf::Post, "confirm('Are you sure?')") %>
    </td>
  </tr>
<% } %>
</table>

</body>
</html>

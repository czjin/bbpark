<!DOCTYPE html>
<%#include "attendantparking.h" %>
<% tfetch(QVariantMap, attendantParking); %>
<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%= controller()->name() + ": " + controller()->activeAction() %></title>
</head>
<body>
<p style="color: red"><%=$ error %></p>
<p style="color: green"><%=$ notice %></p>

<h1>Editing Attendant Parking</h1>

<%== formTag(urla("save", attendantParking["id"]), Tf::Post) %>
  <p>
    <label>ID<br /><input type="text" name="attendantParking[id]" value="<%= attendantParking["id"] %>" readonly="readonly" /></label>
  </p>
  <p>
    <label>Attendant ID<br /><input type="text" name="attendantParking[attendantId]" value="<%= attendantParking["attendantId"] %>" /></label>
  </p>
  <p>
    <label>Parking ID<br /><input type="text" name="attendantParking[parkingId]" value="<%= attendantParking["parkingId"] %>" /></label>
  </p>
  <p>
    <label>Status<br /><input type="text" name="attendantParking[status]" value="<%= attendantParking["status"] %>" /></label>
  </p>
  <p>
    <input type="submit" value="Update" />
  </p>
</form>

<%== linkTo("Show", urla("show", attendantParking["id"])) %> |
<%== linkTo("Back", urla("index")) %>
</body>
</html>
